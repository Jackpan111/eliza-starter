    "express": "^4.18.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
