{
  "name": "@elizaos/eliza-starter",
  "version": "0.1.9",
  "private": true,
  "type": "module",
  "description": "Eliza-starter – chat server dla agenta AI wspierającego dział handlowy transportu ponadgabarytowego",
  "main": "dist/chat-server.js",
  "scripts": {
